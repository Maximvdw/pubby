@prefix conf: <http://richard.cyganiak.de/2007/pubby/config.rdf#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<> a conf:Configuration;
    conf:projectName "Linked Open Vocabularies";
    conf:projectHomepage <http://lov.okfn.org>;
    conf:webBase <http://localhost:8080/>;
#    conf:indexResource <http://localhost:8080/resource/DBpedia>;
    conf:dataset [
        conf:sparqlEndpoint <http://lov.okfn.org/endpoint/lov_aggregator>;
        conf:browsableNamespace <http://>;
        conf:supportsSPARQL11 true;
        conf:queryParamGraph "format=N3";
        conf:resourceDescriptionQuery """
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
CONSTRUCT { ?__this__ ?p ?o }
WHERE {
    {
        GRAPH ?g {
            ?__this__ ?p ?o.
            ?__this__ rdfs:isDefinedBy ?g.
        }
    } UNION {
        GRAPH ?__this__ {
            ?__this__ ?p ?o.
        }
    } UNION {
        GRAPH <http://lov.okfn.org/dataset/lov/stats/lov_metrics.n3> {
            ?__this__ ?p ?o.
        }
    } UNION {
        GRAPH <http://lov.okfn.org/dataset/lov/lov.rdf> {
            ?__this__ ?p ?o.
        }
    }
    FILTER (?p NOT IN ?__high_outdegree_properties__)
}
            """, """
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
CONSTRUCT { ?s ?p ?__this__ }
WHERE {
    {
        GRAPH ?g {
            ?s ?p ?__this__.
            ?__this__ rdfs:isDefinedBy ?g.
        }
    } UNION {
        GRAPH ?__this__ {
            ?s ?p ?__this__.
        }
    } UNION {
        GRAPH <http://lov.okfn.org/dataset/lov/stats/lov_metrics.n3> {
            ?s ?p ?__this__.
        }
    } UNION {
        GRAPH <http://lov.okfn.org/dataset/lov/lov.rdf> {
            ?s ?p ?__this__.
        }
    }
    FILTER (?p NOT IN ?__high_indegree_properties__)
}
            """;
    ];
    .
