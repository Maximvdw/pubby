@prefix papp:    <http://vocab.deri.ie/papp#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix conf:    <http://richard.cyganiak.de/2007/pubby/config.rdf#> .

papp:PlanningAuthority rdfs:label "planning authority"@en.
papp:PlanningApplication rdfs:label "planning application"@en.
papp:Status rdfs:label "status"@en.

papp:planningReference
    rdfs:label "planning reference"@en;
    conf:weight -45.
papp:location
    rdfs:label "location"@en;
    conf:weight -39.
papp:received
    rdfs:label "date received"@en;
    conf:weight -16.
papp:decisionDue
    rdfs:label "date decision due"@en;
    conf:weight -15.
papp:status a conf:HighIndegreeProperty;
    rdfs:label "status"@en;
    owl:inverseOf [ conf:pluralLabel "planning applications"@en; ];
    conf:weight -12.
papp:appeal a conf:HighIndegreeProperty;
    rdfs:label "appeal"@en;
    owl:inverseOf [ conf:pluralLabel "appeals"@en; ];
    conf:weight -11.
papp:planningFile
    rdfs:label "online planning file"@en;
    conf:weight 1.
papp:authority a conf:HighIndegreeProperty;
    rdfs:label "authority"@en;
    owl:inverseOf [ conf:pluralLabel "planning applications"@en; ];
    conf:weight -50.
